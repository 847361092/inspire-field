{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "outputDirectory": "dist",
  "framework": "vue",
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/artworks/upload-handler",
      "destination": "/api/artworks/upload-handler.js"
    },
    {
      "source": "/api/artworks/presign",
      "destination": "/api/artworks/presign.js"
    },
    {
      "source": "/api/artworks/:id",
      "destination": "/api/artworks/[id].js"
    },
    {
      "source": "/api/artworks",
      "destination": "/api/artworks.js"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}